{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(./tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "verbose": true,
  "silent": false,
  "moduleNameMapper": {
    "axios": "axios/dist/node/axios.cjs"
  }
}
